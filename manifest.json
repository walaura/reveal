{
	"manifest_version": 2,

	"name": "Reveal",
	"description": "Adds drama.",
	"version": "1.0",
	"icons": {
		"90": "icon.png"
	},
	"permissions": ["<all_urls>"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["draggable.js"]
		}
	]
}
